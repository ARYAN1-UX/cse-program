#include <stdio.h>

#define MAX_CHAR 256 // Total ASCII characters

void findFrequency(const char *str) {
    int frequency[MAX_CHAR] = {0}; 

    for (int i = 0; str[i] != '\0'; i++) {
        frequency[(unsigned char)str[i]]++;
    }

