#include <stdio.h>

int main() {
    int n, i, j, count;
  
    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);

    int arr[n];
    int frequency[n];  
    int visited[n];  

  printf("Enter the elements of the array:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        frequency[i] = -1;  
        visited[i] = 0;     
    }

 
    for (i = 0; i < n; i++) {
        if (visited[i] == 0) {
            count = 1;
            for (j = i + 1; j < n; j++) {
                if (arr[i] == arr[j]) {
                    count++;
                    visited[j] = 1; 
                }
            }
            frequency[i] = count;
        }
    }

    printf("Element\tFrequency\n");
    for (i = 0; i < n; i++) {
        if (frequency[i] != -1) { 
            printf("%d\t%d\n", arr[i], frequency[i]);
        }
    }

    return 0;
}
